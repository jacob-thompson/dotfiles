source ~/.keys

alias vim="nvim"
alias zshrc="vim ~/.zshrc"
alias aliases="zshrc"

alias cfg="vim ~/.config"
alias libcfg="vim ~/Library/'Application Support'"
alias nvimcfg="vim ~/.config/nvim"

alias serve="jekyll serve -l -H localhost"

alias documents="cd ~/Documents"
alias school="cd ~/Dropbox/school"

# set neovim to be default text editor
export EDITOR="nvim"

# prevent clangd from automatically putting
## unnecessary headers at the top of source files
export CLANGD_FLAGS="--header-insertion=never"

